{
  "product": "rainfall_northern_italy",
  "description": "Rainfall parametric insurance demo â€” Northern Italy, synthetic data, >=150 mm threshold, 30 km radius",

  "bbox": {
    "min_lat": 44.0,
    "max_lat": 47.0,
    "min_lon": 7.0,
    "max_lon": 13.0
  },

  "time_start": "2005-01-01",
  "time_end":   "2025-01-01",

  "grid_step": 0.10,

  "trigger_rule": "geo_radius_threshold",

  "pricing_params": {
    "threshold": 150.0,
    "R_km":      30.0,
    "T_years":   1.0,
    "L":         5000.0,
    "alpha":     0.25,
    "T_hist_years": 20.0
  },

  "fetcher": {
    "name": "synthetic_rainfall",
    "params": {
      "n_stations": 40,
      "n_events": 800,
      "seed": 42
    }
  },

  "outputs": {
    "geojson": "output/rainfall_premium_map.geojson",
    "html":    "output/rainfall_premium_map.html",
    "csv":     "output/rainfall_premium_map.csv"
  }
}
